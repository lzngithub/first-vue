{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": [
    "plugin:vue/essential",
    "google"
  ],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "plugins": [
    "vue"
  ],
  "rules": {
    "linebreak-style": [0 ,"error", "windows"],
    "no-trailing-spaces": "error",
    "semi-spacing": 1,
    "vue/no-parsing-error": [2, { "x-invalid-end-tag": false }]
  }
}